pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
				"java -jar /myproject-0.0.1-SNAPSHOT.jar".execute().text
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
				"java -jar /myproject-0.0.1-SNAPSHOT.jar".execute().text
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
				"java -jar /myproject-0.0.1-SNAPSHOT.jar".execute().text
            }
        }
    }
    }



    
